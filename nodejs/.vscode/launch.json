{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Server (node)",
      "program": "${workspaceFolder}/NODE_JS_WITH_EXPRESS/server.js",
      "cwd": "${workspaceFolder}/NODE_JS_WITH_EXPRESS",
      "runtimeArgs": ["--nolazy"],
      "skipFiles": ["<node_internals>/**"],
      "console": "integratedTerminal",
      "env": { "NODE_ENV": "development" }
    }
  ]
}
